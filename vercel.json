{
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/deposits",
      "destination": "https://api.team33.mx/api/deposits"
    },
    {
      "source": "/api/deposits/:path*",
      "destination": "https://api.team33.mx/api/deposits/:path*"
    },
    {
      "source": "/api/withdrawals",
      "destination": "https://api.team33.mx/api/withdrawals"
    },
    {
      "source": "/api/withdrawals/:path*",
      "destination": "https://api.team33.mx/api/withdrawals/:path*"
    },
    {
      "source": "/api/banks",
      "destination": "https://api.team33.mx/api/banks"
    },
    {
      "source": "/api/banks/:path*",
      "destination": "https://api.team33.mx/api/banks/:path*"
    },
    {
      "source": "/api/wallets",
      "destination": "https://api.team33.mx/api/wallets"
    },
    {
      "source": "/api/wallets/:path*",
      "destination": "https://api.team33.mx/api/wallets/:path*"
    },
    {
      "source": "/api/chat",
      "destination": "https://api.team33.mx/api/chat"
    },
    {
      "source": "/api/chat/:path*",
      "destination": "https://api.team33.mx/api/chat/:path*"
    },
    {
      "source": "/auth/keycloak/:path*",
      "destination": "https://k8s-team33-keycloak-320152ed2f-65380cdab2265c8a.elb.ap-southeast-2.amazonaws.com/:path*"
    },
    {
      "source": "/api/games",
      "destination": "https://api.team33.mx/api/games"
    },
    {
      "source": "/api/games/:path*",
      "destination": "https://api.team33.mx/api/games/:path*"
    },
    {
      "source": "/((?!api/|auth/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, X-API-Key, Authorization" }
      ]
    },
    {
      "source": "/auth/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,POST" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ]
}
